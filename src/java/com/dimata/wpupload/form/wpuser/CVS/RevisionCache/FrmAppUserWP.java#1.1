package com.dimata.wpupload.form.wpuser;

import com.dimata.qdep.form.FRMHandler;
import com.dimata.qdep.form.I_FRMInterface;
import com.dimata.qdep.form.I_FRMType;
import com.dimata.wpupload.entity.wpuser.AppUserWP;
import javax.servlet.http.HttpServletRequest;

/**
 *
 * @author Ardiadi
 */
public class FrmAppUserWP extends FRMHandler implements I_FRMInterface, I_FRMType {
  private AppUserWP entAppUserWP;
  public static final String FRM_NAME_APPUSERWP = "FRM_NAME_APPUSERWP";
  public static final int FRM_FIELD_USER_ID = 0;
  public static final int FRM_FIELD_USER_NAMA = 1;
  public static final int FRM_FIELD_PASSWORD = 2;
  public static final int FRM_FIELD_CREATE_DATE = 3;
  public static final int FRM_FIELD_LAST_LOGIN = 4;
  public static final int FRM_FIELD_NIP = 5;
  public static final int FRM_FIELD_KD_JABATAN = 6;
  public static final int FRM_FIELD_NPWPD = 7;
  public static final int FRM_FIELD_KD_JENIS_USAHA = 8;
  public static final int FRM_FIELD_KD_JENIS_WP = 9;
  public static final int FRM_FIELD_KD_SUBJENIS_WP = 10;
  public static final int FRM_FIELD_KD_LOKASI = 11;
  public static final int FRM_FIELD_KD_SUBLOKASI = 12;
  public static final int FRM_FIELD_NO_TELP = 13;

public static String[] fieldNames = {
    "FRM_FIELD_USER_ID",
    "FRM_FIELD_USER_NAMA",
    "FRM_FIELD_PASSWORD",
    "FRM_FIELD_CREATE_DATE",
    "FRM_FIELD_LAST_LOGIN",
    "FRM_FIELD_NIP",
    "FRM_FIELD_KD_JABATAN",
    "FRM_FIELD_NPWPD",
    "FRM_FIELD_KD_JENIS_USAHA",
    "FRM_FIELD_KD_JENIS_WP",
    "FRM_FIELD_KD_SUBJENIS_WP",
    "FRM_FIELD_KD_LOKASI",
    "FRM_FIELD_KD_SUBLOKASI",
    "FRM_FIELD_NO_TELP"
};

public static int[] fieldTypes = {
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_DATE,
    TYPE_DATE,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING,
    TYPE_STRING
};

public FrmAppUserWP() {
}

public FrmAppUserWP(AppUserWP entAppUserWP) {
this.entAppUserWP = entAppUserWP;
}

public FrmAppUserWP(HttpServletRequest request, AppUserWP entAppUserWP) {
super(new FrmAppUserWP(entAppUserWP), request);
this.entAppUserWP = entAppUserWP;
}

public String getFormName() {
return FRM_NAME_APPUSERWP;
}

public int[] getFieldTypes() {
return fieldTypes;
}

public String[] getFieldNames() {
return fieldNames;
}

public int getFieldSize() {
return fieldNames.length;
}

public AppUserWP getEntityObject() {
return entAppUserWP;
}

public void requestEntityObject(AppUserWP entAppUserWP) {
try {
this.requestParam();
    entAppUserWP.setWpUserId(getString(FRM_FIELD_USER_ID));
    entAppUserWP.setWpUserNama(getString(FRM_FIELD_USER_NAMA));
    entAppUserWP.setWpUserPassword(getString(FRM_FIELD_PASSWORD));
    entAppUserWP.setWpUserCreateDate(getDate(FRM_FIELD_CREATE_DATE));
    entAppUserWP.setWpUserLastLogin(getDate(FRM_FIELD_LAST_LOGIN));
    entAppUserWP.setWpUserNIP(getString(FRM_FIELD_NIP));
    entAppUserWP.setWpUserKDJabatan(getString(FRM_FIELD_KD_JABATAN));
    entAppUserWP.setWpUserNPWPD(getString(FRM_FIELD_NPWPD));
    entAppUserWP.setWpUserKDJenisUsaha(getString(FRM_FIELD_KD_JENIS_USAHA));
    entAppUserWP.setWpUserKDJenisWP(getString(FRM_FIELD_KD_JENIS_WP));
    entAppUserWP.setWpUserKDSubjenisWP(getString(FRM_FIELD_KD_SUBJENIS_WP));
    entAppUserWP.setWpUserLokasi(getString(FRM_FIELD_KD_LOKASI));
    entAppUserWP.setWpUserSublokasi(getString(FRM_FIELD_KD_SUBLOKASI));
    entAppUserWP.setWpUserNoTelp(getString(FRM_FIELD_NO_TELP));
} catch (Exception e) {
System.out.println("Error on requestEntityObject : " + e.toString());
}
}

}
