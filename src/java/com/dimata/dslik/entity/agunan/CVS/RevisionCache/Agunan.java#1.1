    @Override
    public String getLogDetail(Entity prevDoc) {
        //throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
        String history="";
        Agunan prevMat = (Agunan)prevDoc;
        if(!prevMat.getFlagDetail().equals(this.getFlagDetail())){
            history=history+" Flag Detail diubah dari "+prevMat.getFlagDetail()+" menjadi "+this.getFlagDetail()+"/n";
        }
        if(!prevMat.getKodeRegisterAgunan().equals(this.getKodeRegisterAgunan())){
            history=history+" Kode Register Agunan diubah dari "+prevMat.getKodeRegisterAgunan()+" menjadi "+this.getKodeRegisterAgunan()+"/n";
        }        
        if(!prevMat.getNoRekening().equals(this.getNoRekening())){
             history=history+" No. Rekening diubah dari "+prevMat.getNoRekening()+" menjadi "+this.getNoRekening()+"/n";
        }
        if(!prevMat.getCif().equals(this.getCif())){
             history=history+" CIF duibah dari dari "+prevMat.getCif()+" menjadi "+this.getCif()+"/n";
        }
        if(!prevMat.getKodeStatusAgunan().equals(this.getKodeStatusAgunan())){
             history=history+" Kode Status Agunan diubah dari "+prevMat.getKodeStatusAgunan()+" menjadi "+this.getKodeStatusAgunan()+"/n";
        }
        if(!prevMat.getKodeJenisAgunan().equals(this.getKodeJenisAgunan())){
             history=history+" Kode Jenis Agunan diubah dari "+prevMat.getKodeJenisAgunan()+" menjadi "+this.getKodeJenisAgunan()+"/n";
