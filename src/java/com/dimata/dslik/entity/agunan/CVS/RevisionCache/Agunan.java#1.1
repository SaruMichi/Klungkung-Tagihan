        if(!prevMat.getKodeLembagaPemeringkat().equals(this.getKodeLembagaPemeringkat())){
             history=history+" Kode Lembaga Pemeringkat diubah dari "+prevMat.getKodeLembagaPemeringkat()+" menjadi "+this.getKodeLembagaPemeringkat()+"/n";
        }
        if(!prevMat.getKodeJenisPengikatan().equals(this.getKodeJenisPengikatan())){
             history=history+" Kode Jenis Pengikatan diubah dari "+prevMat.getKodeJenisPengikatan()+" menjadi "+this.getKodeJenisPengikatan()+"/n";
        }
        if(!prevMat.getTglPengikatan().equals(this.getTglPengikatan())){
            history=history+" Tanggal Pengikatan diubah dari "+prevMat.getTglPengikatan()+" menjadi "+this.getTglPengikatan()+"/n";
        }
        if(!prevMat.getNamaPemilikAgunan().equals(this.getNamaPemilikAgunan())){
            history=history+" Nama Pemilik Agunan diubah dari "+prevMat.getNamaPemilikAgunan()+" menjadi "+this.getTglPengikatan()+"/n";
        }
        if(!prevMat.getBuktiKepemilikan().equals(this.getBuktiKepemilikan())){
            history=history+" Bukti Kepemilikan diubah dari "+prevMat.getBuktiKepemilikan()+" menjadi "+this.getBuktiKepemilikan()+"/n";
        }
        if(!prevMat.getAlamatAgunan().equals(this.getAlamatAgunan())){
            history=history+" Alamat Agunan diubah dari "+prevMat.getAlamatAgunan()+" menjadi "+this.getAlamatAgunan()+"/n";
        }
        if(!prevMat.getKodeKabLokasiAgunan().equals(this.getKodeKabLokasiAgunan())){
            history=history+" Kode Kab/Kota (DATI 2) Lokasi Agunan diubah dari "+prevMat.getKodeKabLokasiAgunan()+" menjadi "+this.getKodeKabLokasiAgunan()+"/n";
        }
        if(!prevMat.getAlamatAgunan().equals(this.getAlamatAgunan())){
            history=history+" Alamat Agunan diubah dari "+prevMat.getAlamatAgunan()+" menjadi "+this.getAlamatAgunan()+"/n";
        }
        if(prevMat.getNilaiAgunanNjop() != this.getNilaiAgunanNjop()){
            history=history+" Nilai Agunan (NJOP) diubah dari "+prevMat.getNilaiAgunanNjop()+" menjadi "+this.getNilaiAgunanNjop()+"/n";
        }
        if(prevMat.getNilaiAgunanLjk() != this.getNilaiAgunanLjk()){
            history=history+" Nilai Agunan Menurut LJK diubah dari "+prevMat.getNilaiAgunanLjk()+" menjadi "+this.getNilaiAgunanLjk()+"/n";
        }
        if(!prevMat.getTglPenilaianLjk().equals(this.getTglPenilaianLjk())){
            history=history+" Tanggal LJK diubah dari "+prevMat.getTglPenilaianLjk()+" menjadi "+this.getTglPenilaianLjk()+"/n";
        }
        if(!prevMat.getNamaPenilaiIndep().equals(this.getNamaPenilaiIndep())){
