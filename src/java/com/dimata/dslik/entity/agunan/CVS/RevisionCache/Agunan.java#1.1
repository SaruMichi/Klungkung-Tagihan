            history=history+" Tanggal Penilaian Penilai Independen diubah dari "+prevMat.getTglPenilaianPenilaiIndep()+" menjadi "+this.getTglPenilaianPenilaiIndep()+"/n";
        }
        if(prevMat.getStatusParipasu() != this.getStatusParipasu()){
            history=history+" Status Paripasu diubah dari "+prevMat.getStatusParipasu()+" menjadi "+this.getStatusParipasu()+"/n";
        }
        if(!prevMat.getStatusKreditJoin().equals(this.getStatusKreditJoin())){
            history=history+" Status Kredit Join diubah dari "+prevMat.getStatusKreditJoin()+" menjadi "+this.getStatusKreditJoin()+"/n";
