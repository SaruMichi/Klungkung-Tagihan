    public void run() {

        System.out.println("start .... ");
    
        while (ManagerDilimitedTextSummary.running) {
            
            try {
                
                /*proses transfer*/
                //cek periode berjalan
                ManagerDilimitedTextSummary.setStatusProses("Start Periode <br>");
                
                //Cek jumlah data
                //buatkan proses pemindahan data sampai dengan proses dilimited text
                
                ManagerDilimitedTextSummary.setStatusProses("Get Debitur <br>");
                
            } catch (Exception e) {
                System.out.println("Interrupted " + e);
            }
            
